package ex2;

public class Main {

	public static void main(String[] args) {
		/*
		 * Tipo 1 - Aquático
		 * Tipo 2 - Terrestre
		 * Tipo 3 - Aéreo
		 */
		Animal animal1 = new Animal(1);
		Animal animal2 = new Animal(2);
		Animal animal3 = new Animal(3);
		
		System.out.printf("O animal 1 é do tipo %s\n", animal1.getTipo());
		System.out.printf("O animal 2 é do tipo %s\n", animal2.getTipo());
	}
}
